import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
 
       Scanner input = new Scanner(System.in);
       int n = input.nextInt();
       int h = input.nextInt();
 
       int ans =0;
 
       int[] arr = new int[n+10];
       for(int i=0;i<n;i++){
           arr[i] = input.nextInt();
           if(arr[i]>h){
               ans = ans+2;
           }else {
               ans+=1;
           }
       }
        System.out.println(ans);
    }
}
